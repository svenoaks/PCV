package de.tinysip.stun;

/**
 * 
 * Implement this interface to listen to events raised by the STUNDiscoveryTask.
 * 
 * @author Sebastian
 * 
 */
public interface STUNDiscoveryResultListener {
	/**
	 * Send a STUNDiscoveryResult event to all registered listeners.
	 * 
	 * @param event
	 *            the event to send
	 */
	public void STUNDiscoveryResultChanged(STUNDiscoveryResultEvent event);

}
